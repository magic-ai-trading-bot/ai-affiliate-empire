{
  "name": "ai-affiliate-empire",
  "version": "1.0.0",
  "description": "Fully autonomous AI-powered affiliate marketing system that generates content, publishes across platforms, and self-optimizes for revenue.",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "semantic-release": "semantic-release",
    "prepare": "husky install || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-affiliate-empire.git"
  },
  "keywords": [
    "ai",
    "affiliate-marketing",
    "autonomous",
    "content-generation",
    "video-automation",
    "youtube-shorts",
    "tiktok",
    "instagram-reels",
    "revenue-automation"
  ],
  "author": "Dung Ngo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/ai-affiliate-empire/issues"
  },
  "homepage": "https://github.com/yourusername/ai-affiliate-empire#readme",
  "files": [
    ".claude/",
    ".opencode/",
    "docs/",
    "plans/",
    "AGENTS.md",
    "CLAUDE.md",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    ".claude/metadata.json"
  ],
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.8",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^9.38.0",
    "husky": "^8.0.3",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^22.0.12",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.920.0",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "^6.18.0",
    "axios": "^1.13.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crypto-js": "^4.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  }
}
