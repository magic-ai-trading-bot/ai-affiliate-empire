{
  "name": "ai-affiliate-empire",
  "version": "1.0.0",
  "description": "Fully autonomous AI-powered affiliate marketing system with Temporal orchestration, multi-platform publishing (YouTube, TikTok, Instagram), and self-optimizing ROI tracking. Built with NestJS, Prisma, and Next.js.",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"dashboard/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" \"dashboard/**/*.{ts,tsx}\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test,dashboard}/**/*.{ts,tsx}\" --max-warnings 0",
    "lint:src": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 100",
    "lint:fix": "eslint \"{src,test,dashboard}/**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "test:unit": "jest --testPathPatterns=test/unit",
    "test:integration": "jest --config test/integration/jest.config.js",
    "test:integration:watch": "jest --config test/integration/jest.config.js --watch",
    "test:integration:coverage": "jest --config test/integration/jest.config.js --coverage",
    "test:integration:workflows": "jest --config test/integration/jest.config.js --testPathPattern=workflows",
    "test:integration:pipelines": "jest --config test/integration/jest.config.js --testPathPattern=pipelines",
    "test:integration:database": "jest --config test/integration/jest.config.js --testPathPattern=database",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:cov": "jest --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:smoke": "jest --config test/smoke/jest.config.js",
    "test:smoke:staging": "STAGING_URL=https://ai-affiliate-empire-staging.fly.dev jest --config test/smoke/jest.config.js",
    "test:smoke:production": "PRODUCTION_URL=https://ai-affiliate-empire.fly.dev jest --config test/smoke/jest.config.js",
    "test:load": "./scripts/run-load-tests.sh all local",
    "test:load:baseline": "k6 run --env BASE_URL=http://localhost:3000 test/load/scenarios/baseline.js",
    "test:load:stress": "./scripts/run-load-tests.sh stress local",
    "test:load:spike": "k6 run --env BASE_URL=http://localhost:3000 test/load/scenarios/spike-test.js",
    "test:load:soak": "./scripts/run-load-tests.sh soak local",
    "test:load:products": "k6 run --env BASE_URL=http://localhost:3000 test/load/scenarios/products-load.js",
    "test:load:analytics": "k6 run --env BASE_URL=http://localhost:3000 test/load/scenarios/analytics-load.js",
    "test:load:orchestrator": "k6 run --env BASE_URL=http://localhost:3000 test/load/scenarios/orchestrator-load.js",
    "test:load:staging": "./scripts/run-load-tests.sh baseline staging",
    "test:load:production": "./scripts/run-load-tests.sh baseline production",
    "test:e2e:playwright": "playwright test",
    "test:e2e:playwright:headed": "playwright test --headed",
    "test:frontend": "npm run test:dashboard && npm run test:blog",
    "test:dashboard": "cd dashboard && npm test",
    "test:blog": "cd apps/blog && npm test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "migrate:secrets": "ts-node scripts/migrate-secrets-to-aws.ts",
    "semantic-release": "semantic-release",
    "prepare": "husky install || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magic-ai-trading-bot/ai-affiliate-empire.git"
  },
  "keywords": [
    "ai",
    "affiliate-marketing",
    "autonomous-system",
    "content-generation",
    "video-automation",
    "youtube-shorts",
    "tiktok",
    "instagram-reels",
    "revenue-automation",
    "nestjs",
    "temporal-workflow",
    "prisma",
    "nextjs",
    "openai-gpt4",
    "roi-tracking",
    "multi-platform-publishing",
    "self-optimizing",
    "docker",
    "typescript",
    "postgresql",
    "social-media-automation"
  ],
  "author": "Dung Ngo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magic-ai-trading-bot/ai-affiliate-empire/issues"
  },
  "homepage": "https://github.com/magic-ai-trading-bot/ai-affiliate-empire#readme",
  "files": [
    ".claude/",
    ".opencode/",
    "docs/",
    "plans/",
    "AGENTS.md",
    "CLAUDE.md",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    ".claude/metadata.json"
  ],
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.8",
    "@playwright/test": "^1.56.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@temporalio/testing": "^1.13.1",
    "@types/bcrypt": "^6.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.5",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^8.0.3",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^22.0.12",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.920.0",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.18.0",
    "@sentry/node": "^10.22.0",
    "@sentry/tracing": "^7.120.4",
    "@temporalio/activity": "^1.13.1",
    "@temporalio/client": "^1.13.1",
    "@temporalio/worker": "^1.13.1",
    "@temporalio/workflow": "^1.13.1",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "fluent-ffmpeg": "^2.1.3",
    "google-trends-api": "^4.9.2",
    "googleapis": "^164.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "nest-winston": "^1.10.2",
    "openai": "^6.7.0",
    "p-queue": "^7.4.1",
    "paapi5-nodejs-sdk": "^1.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "snoowrap": "^1.23.0",
    "twitter-api-v2": "^1.27.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
